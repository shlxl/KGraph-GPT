{
  "name": "GraphRAG-RELSET-AI-RAG-zh",
  "version": "2025-11-08",
  "types": [
    "Model",
    "EmbeddingModel",
    "Retriever",
    "Reranker",
    "Generator",
    "Pipeline",
    "Component",
    "Dataset",
    "Corpus",
    "Document",
    "Chunk",
    "Graph",
    "GraphDB",
    "VectorIndex",
    "VectorStore",
    "Task",
    "Benchmark",
    "Metric",
    "Paper",
    "Library",
    "Prompt",
    "Tool",
    "Query",
    "Claim",
    "Value",
    "Location",
    "Event"
  ],
  "relations": [
    { "name": "implements", "domain": ["Pipeline","Component"], "range": ["Task","Algorithm","Pattern"], "symmetric": false },
    { "name": "uses_component", "domain": ["Pipeline"], "range": ["Retriever","Reranker","Generator","Graph","GraphDB","VectorIndex","VectorStore","EmbeddingModel","Prompt","Tool"], "symmetric": false },
    { "name": "embeds_with", "domain": ["Pipeline","Corpus","Document","Chunk"], "range": ["EmbeddingModel"], "symmetric": false },
    { "name": "indexes_with", "domain": ["VectorStore"], "range": ["VectorIndex"], "symmetric": false },
    { "name": "stored_in", "domain": ["EmbeddingModel","Document","Chunk","Graph"], "range": ["VectorStore","GraphDB"], "symmetric": false },
    { "name": "retrieves_with", "domain": ["Pipeline","Retriever"], "range": ["VectorStore","Graph"], "symmetric": false },
    { "name": "re_ranks_with", "domain": ["Pipeline","Retriever"], "range": ["Reranker"], "symmetric": false },
    { "name": "answers_with", "domain": ["Pipeline"], "range": ["Generator"], "symmetric": false },
    { "name": "prompts_with", "domain": ["Pipeline","Generator"], "range": ["Prompt"], "symmetric": false },
    { "name": "augments_with", "domain": ["Pipeline","Generator"], "range": ["Retriever","Graph"], "symmetric": false },
    { "name": "builds_graph_from", "domain": ["Pipeline","Graph"], "range": ["Corpus","Document"], "symmetric": false },
    { "name": "contains_node_type", "domain": ["Graph"], "range": ["Value"], "symmetric": false },
    { "name": "contains_edge_type", "domain": ["Graph"], "range": ["Value"], "symmetric": false },
    { "name": "summarizes_with", "domain": ["Graph"], "range": ["Algorithm","Prompt","Model"], "symmetric": false },
    { "name": "grounded_by", "domain": ["Pipeline","Generator","Claim"], "range": ["Document","Chunk","Graph"], "symmetric": false },
    { "name": "evidence_from", "domain": ["Answer","Claim"], "range": ["Document","Chunk","Graph"], "symmetric": false },
    { "name": "chunked_into", "domain": ["Corpus","Document"], "range": ["Chunk"], "symmetric": false },
    { "name": "trains_on", "domain": ["Model"], "range": ["Dataset","Corpus"], "symmetric": false },
    { "name": "fine_tunes_from", "domain": ["Model"], "range": ["Model"], "symmetric": false },
    { "name": "evaluates_on", "domain": ["Pipeline","Model"], "range": ["Benchmark","Dataset","Task"], "symmetric": false },
    { "name": "reports_metric", "domain": ["Pipeline","Model"], "range": ["Metric","Value"], "symmetric": false },
    { "name": "cites", "domain": ["Paper","Library"], "range": ["Paper","Dataset","Benchmark"], "symmetric": false },
    { "name": "released_in", "domain": ["Paper","Library","Model"], "range": ["Conference","Journal","Repository"], "symmetric": false },
    { "name": "alias_of", "domain": ["*"], "range": ["*"], "symmetric": true },
    { "name": "not_same_as", "domain": ["*"], "range": ["*"], "symmetric": true },
    { "name": "denies", "domain": ["Organization","Person","Pipeline"], "range": ["Claim","Event","Value"], "symmetric": false },
    { "name": "rumor_about", "domain": ["Claim","Event","Value"], "range": ["Pipeline","Model","Library","Dataset"], "symmetric": false },
    { "name": "caused", "domain": ["Event","Tool","Pipeline"], "range": ["Event"], "symmetric": false },
    { "name": "caused_by", "domain": ["Event"], "range": ["Tool","Pipeline","Event"], "symmetric": false }
  ],
  "negation_prefix": "negated:",
  "retrieval_enums": {
    "retrieval_strategy": ["bm25","dense","sparse","hybrid","graph_walk","community_summary","path_ranking"],
    "distance_fn": ["ip","cosine","l2"],
    "index_type": ["flat","ivf","ivf_pq","ivf_hnsw","hnsw","diskann","scann","annoy","hnswlib"],
    "graph_algo": ["pagerank","hits","bfs","dfs","k_core","community_louvain","community_leiden"]
  },
  "qualifiers": {
    "since": "YYYY-MM[-DD]",
    "until": "YYYY-MM[-DD]",
    "date": "YYYY-MM[-DD]",
    "version": "string",
    "arxiv_id": "string",
    "repo": "string",
    "license": "string",
    "topk": "integer",
    "hops": "integer",
    "path_length": "integer",
    "chunk_size": "integer",
    "chunk_overlap": "integer",
    "window_size": "integer",
    "retrieval_strategy": "enum (see retrieval_enums.retrieval_strategy)",
    "index_type": "enum (see retrieval_enums.index_type)",
    "distance_fn": "enum (see retrieval_enums.distance_fn)",
    "graph_algo": "enum (see retrieval_enums.graph_algo)",
    "temperature": "number",
    "max_tokens": "integer",
    "latency_ms": "number",
    "throughput_qps": "number",
    "cost_usd": "number",
    "accuracy": "number",
    "faithfulness": "number",
    "groundedness": "number",
    "hallucination_rate": "number",
    "f1": "number",
    "recall": "number",
    "precision": "number",
    "mrr": "number",
    "hit_rate": "number",
    "map": "number",
    "nDCG": "number",
    "metric_name": "string",
    "split": "enum[train,dev,test]",
    "role": "string",
    "alias": "string",
    "note": "string"
  },
  "evidence_schema": {
    "doc": "<DOC_ID>",
    "sents": [1,2]
  },
  "output": {
    "format": "JSONL",
    "example": [
      {
        "head": { "text": "Pipeline.RAG-Alpha", "type": "Pipeline" },
        "relation": "uses_component",
        "tail": { "text": "Retriever.Hybrid(BM25+dense)", "type": "Retriever" },
        "qualifiers": { "retrieval_strategy": "hybrid", "topk": 20 },
        "evidence": [{ "doc": "spec_rag_alpha", "sents": [12,13] }],
        "confidence": 0.86
      },
      {
        "head": { "text": "Retriever.Hybrid(BM25+dense)", "type": "Retriever" },
        "relation": "retrieves_with",
        "tail": { "text": "VectorStore.FAISS", "type": "VectorStore" },
        "qualifiers": { "index_type": "hnsw", "distance_fn": "cosine" },
        "evidence": [{ "doc": "infra_notes", "sents": [3] }],
        "confidence": 0.84
      },
      {
        "head": { "text": "Pipeline.GraphRAG-Beta", "type": "Pipeline" },
        "relation": "builds_graph_from",
        "tail": { "text": "Corpus.ProductDocs", "type": "Corpus" },
        "qualifiers": { "graph_algo": "community_louvain", "hops": 2 },
        "evidence": [{ "doc": "design_graphrag_beta", "sents": [7,9] }],
        "confidence": 0.83
      },
      {
        "head": { "text": "Pipeline.GraphRAG-Beta", "type": "Pipeline" },
        "relation": "augments_with",
        "tail": { "text": "Graph.ProductKG", "type": "Graph" },
        "qualifiers": { "path_length": 3 },
        "evidence": [{ "doc": "design_graphrag_beta", "sents": [10] }],
        "confidence": 0.82
      },
      {
        "head": { "text": "Answer.Q123", "type": "Value" },
        "relation": "grounded_by",
        "tail": { "text": "Chunk.d42", "type": "Chunk" },
        "qualifiers": { "note": "multi-hop reasoning across KG path" },
        "evidence": [{ "doc": "runlog_2025-11-01", "sents": [21,22] }],
        "confidence": 0.81
      },
      {
        "head": { "text": "Pipeline.RAG-Alpha", "type": "Pipeline" },
        "relation": "evaluates_on",
        "tail": { "text": "Benchmark.BEIR-subset", "type": "Benchmark" },
        "qualifiers": { "split": "test" },
        "evidence": [{ "doc": "eval_plan", "sents": [4] }],
        "confidence": 0.80
      },
      {
        "head": { "text": "Pipeline.RAG-Alpha", "type": "Pipeline" },
        "relation": "reports_metric",
        "tail": { "text": "Metric.nDCG@10", "type": "Metric" },
        "qualifiers": { "nDCG": 0.58, "mrr": 0.44, "faithfulness": 0.91, "groundedness": 0.94 },
        "evidence": [{ "doc": "eval_report", "sents": [8,9] }],
        "confidence": 0.80
      }
    ]
  },
  "policies": {
    "evidence_min_unit": "sentence",
    "allow_multi_sentence_evidence": true,
    "drop_facts_without_verifiable_evidence": true,
    "use_alias_of_for_synonyms_only": true,
    "use_not_same_as_for_name_collision": true,
    "negation_as_prefix_only": true
  },
  "authority_order_for_disputes": [
    "ConferenceOrJournalPaper",
    "OfficialRepositoryOrDocs",
    "BenchmarkMaintainer",
    "MajorVendorDocs",
    "ResearchBlog",
    "CommunityRepo",
    "SocialMedia"
  ]
}
